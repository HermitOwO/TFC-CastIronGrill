{
  "multipart": [
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "castirongrill:block/cast_iron_grill_firepit"
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "castirongrill:block/cast_iron_grill_firepit",
        "y": 90
      }
    },
    {
      "when": {
        "lit": true,
        "axis": "x"
      },
      "apply": {
        "model": "tfc:block/firepit_lit_low"
      }
    },
    {
      "when": {
        "lit": true,
        "axis": "z"
      },
      "apply": {
        "model": "tfc:block/firepit_lit_low",
        "y": 90
      }
    },
    {
      "when": {
        "lit": false,
        "axis": "x"
      },
      "apply": {
        "model": "tfc:block/firepit_unlit"
      }
    },
    {
      "when": {
        "lit": false,
        "axis": "z"
      },
      "apply": {
        "model": "tfc:block/firepit_unlit",
        "y": 90
      }
    }
  ]
}